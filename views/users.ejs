<% include partials/header %>
<div class="container">
    <div class="docs-example">
        <br>
        <h2 class="docs-header">Users</h2>
        <br>
            <table class="u-max-full-width">
                <thead>
                    <tr>
                        <th>User</th>
                        <th>Email </th>
                        <th> ID </th>
                        <th>More</th>
                    </tr>
                </thead>
            <% users.forEach(function(user) { %>
            <tbody>
            <tr>
                <td><%= user.username.toUpperCase() %></td>
                <td><a href="mailto:<%= user.email %>"><%= user.email.toUpperCase()%></a></td>
                <td><%= user._id %></td>
                <td><a href="/users/<%= user._id %>" class="btn btn-primary">More info</a></td>
            </tr>
            <% }); %>
            </tbody>
        </table>
    </div>
</div>
<% include partials/footer %>